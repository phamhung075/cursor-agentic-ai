    "gray-matter": "^4.0.3",
    "marked": "^5.1.1",
    "openai": "^4.28.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "ai",
    "agent",
    "self-improvement",
    "automation",
    "coding-framework",
    "analysis",
    "modular",
    "organized"
