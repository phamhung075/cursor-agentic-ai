{
  "name": "agentic-coding-framework-self-improvement",
  "version": "2.0.0",
  "description": "Modular self-improving AI agent for the Agentic Coding Framework with Pinecone memory",
  "main": "agents/self-improvement/index.js",
  "scripts": {
    "AAI:start": "node agents/self-improvement/index.js",
    "AAI:agent": "node agents/self-improvement/index.js",
    "AAI:analyze": "node agents/self-improvement/index.js",
    "AAI:improve": "node agents/self-improvement/index.js",
    "AAI:legacy": "node agents/_store/scripts/self_improvement_agent_legacy.js",
    "AAI:demo": "node agents/_store/scripts/demo_agent.js",
    "AAI:install-deps": "npm install",
    "AAI:test": "echo \"Tests to be implemented\" && exit 0",
    "AAI:test-agent": "node agents/_store/tests/test_new_agent.js",
    "AAI:test-integration": "node agents/_store/tests/test_autopilot_integration.js",
    "AAI:test-system": "node agents/_store/tests/test_new_system.js",
    "AAI:test-dependencies": "node agents/_store/tests/test_dependency_tracking.js",
    "AAI:setup-env": "node agents/_store/scripts/setup-env.js",
    "AAI:install-to": "node agents/_store/scripts/install-agent-ai.js",
    "AAI:test-startup": "node agents/self-improvement/index.js --test",
    "AAI:sync-memory": "node agents/_store/scripts/sync-memory.js",
    "AAI:sync-status": "node agents/_store/scripts/sync-memory.js status",
    "AAI:sync-up": "node agents/_store/scripts/sync-memory.js up",
    "AAI:sync-down": "node agents/_store/scripts/sync-memory.js down",
    "AAI:sync-both": "node agents/_store/scripts/sync-memory.js both",
    "AAI:fix-embeddings": "node agents/_store/scripts/sync-memory.js fix-embeddings",
    "AAI:integrate": "node agents/_store/projects/integrate_agents_architecture.js",
    "AAI:fix-paths": "node agents/_store/projects/comprehensive_backup_and_fix.js",
    "AAI:core-framework": "echo 'Core framework located at: agents/_store/projects/_core' && ls -la agents/_store/projects/_core",
    "AAI:core-status": "node -e \"console.log('📊 Core Framework Status:'); const fs = require('fs'); const path = 'agents/_store/projects/_core'; const rules = fs.readdirSync(path + '/rules', {recursive: true}).filter(f => f.endsWith('.mdc')); console.log('📁 Total .mdc files:', rules.length); console.log('🔧 Workflow files:', fs.readdirSync(path + '/rules/01__AI-RUN').filter(f => f.endsWith('.mdc')).length); console.log('📝 Template files:', fs.readdirSync(path + '/rules/02__AI-DOCS', {recursive: true}).filter(f => f.endsWith('.mdc')).length); console.log('📋 Spec files:', fs.readdirSync(path + '/rules/03__SPECS', {recursive: true}).filter(f => f.endsWith('.mdc')).length);\"",
    "AAI:analyze-core": "node agents/_store/projects/core_path_analyzer.js",
    "AAI:fix-core": "node agents/_store/projects/core_path_fixer.js",
    "AAI:analyze-and-fix": "npm run AAI:analyze-core && npm run AAI:fix-core",
    "AAI:core-health": "npm run AAI:analyze-core && echo '\\n🔧 To fix issues, run: npm run AAI:fix-core'",
    "AAI:backup-core": "bash agents/_store/scripts/backup_core_structure.sh",
    "AAI:update-file-list": "python3 agents/_store/scripts/update_core_files_list.py",
    "AAI:clean-scripts": "node agents/_store/scripts/cleanup_obsolete_scripts.js",
    "AAI:scripts-help": "node agents/_store/scripts/script_manager.js help",
    "AAI:scripts-migrate": "node agents/_store/scripts/migrate_scripts_to_store.js",
    "AAI:scripts-list": "node agents/_store/scripts/script_manager.js list",
    "AAI:scripts-organize": "node agents/_store/scripts/script_manager.js organize",
    "AAI:scripts-analyze": "node agents/_store/scripts/script_manager.js analyze",
    "AAI:scripts-categories": "node agents/_store/scripts/script_manager.js categories",
    "AAI:core-scan": "node agents/_store/scripts/core/core_file_manager.js scan",
    "AAI:core-report": "node agents/_store/scripts/core/core_file_manager.js report",
    "AAI:core-search": "node agents/_store/scripts/core/core_file_manager.js search",
    "AAI:core-monitor": "node agents/_store/scripts/core/core_file_monitor.js start",
    "AAI:core-monitor-status": "node agents/_store/scripts/core/core_file_monitor.js status",
    "AAI:core-api": "node agents/_store/scripts/core/core_file_api.js",
    "AAI:core-api-status": "node agents/_store/scripts/core/core_file_api.js status",
    "AAI:core-validate": "node agents/_store/scripts/core/core_file_monitor.js validate"
  },
  "dependencies": {
    "@pinecone-database/pinecone": "^2.0.1",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "dotenv": "^16.5.0",
    "gray-matter": "^4.0.3",
    "marked": "^5.1.1",
    "openai": "^4.28.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "ai",
    "agent",
    "self-improvement",
    "automation",
    "coding-framework",
    "analysis",
    "modular",
    "organized"
  ],
  "author": "Agentic Coding Framework",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "bin": {
    "self-improvement-agent": "./agents/self-improvement/index.js"
  }
}