{
  "name": "agentic-coding-framework-self-improvement",
  "version": "2.0.0",
  "description": "Modular self-improving AI agent for the Agentic Coding Framework with Pinecone memory",
  "main": "agents/self-improvement/index.js",
  "scripts": {
    "launch": "node agents/_store/scripts/launch-aai-complete.js",
    "AAI:cleanup": "node agents/_store/scripts/comprehensive-cleanup.js",
    "AAI:memory-cleanup": "node agents/_store/scripts/memory-cleanup.js",
