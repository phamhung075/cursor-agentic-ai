    "AAI:context-track": "node agents/_store/scripts/smart-context-tracker.js",
    "AAI:performance-optimize": "node agents/_store/scripts/performance-optimizer.js",
    "AAI:memory-index": "cat agents/_store/memory/index.json",
    "AAI:sync-preserved": "node agents/_store/scripts/sync-preserved-to-memory.js",
    "AAI:sync-preserved-status": "node agents/_store/scripts/sync-preserved-to-memory.js status",
