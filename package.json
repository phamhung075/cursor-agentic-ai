    "AAI:test-dependencies": "node agents/_store/tests/test_dependency_tracking.js",
    "AAI:setup-env": "node agents/_store/scripts/setup-env.js",
    "AAI:install-to": "node agents/_store/scripts/install-agent-ai.js",
    "AAI:sync-memory": "node agents/_store/scripts/sync-memory.js",
    "AAI:sync-status": "node agents/_store/scripts/sync-memory.js status",
    "AAI:sync-up": "node agents/_store/scripts/sync-memory.js up",
    "AAI:sync-down": "node agents/_store/scripts/sync-memory.js down",
    "AAI:sync-both": "node agents/_store/scripts/sync-memory.js both",
