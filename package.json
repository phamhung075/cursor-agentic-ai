    "AAI:core-monitor-status": "node agents/_store/scripts/core/core_file_monitor.js status",
    "src:list": "node src/scripts/project_manager.js list",
    "src:add-project": "node src/scripts/project_manager.js add",
    "src:update": "node src/scripts/project_manager.js update",
    "src:build": "node src/scripts/project_manager.js build",
    "src:docs": "node src/scripts/project_manager.js docs",
    "src:help": "node src/scripts/project_manager.js help",
    "cursor:setup": "node agents/_store/scripts/simple-cursor-setup.js",
